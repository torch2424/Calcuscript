{
  "name": "CrunchKey",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Simple Text Based Calculator",
  "homepage_url": "http://aaronturnerapps.tumblr.com",
  "icons": {
    "16": "icons/CrunchKey16.png",
    "48": "icons/CrunchKey48.png",
    "128": "icons/CrunchKey128.png"
  },
  "default_locale": "en",
  "background": 
    {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": 
{
    "default_icon": "icons/CrunchKey19.png",
    "default_title": "CrunchKey",
    "default_popup": "src/browser_action/browser_action.html"
}
}
